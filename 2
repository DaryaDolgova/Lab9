cout<<"N= "; cin>>n;
if(c=='s'){
if(n==-1){cout<<"vostok"<<ends;}
if(n==1){cout<<"zapad"<<ends;}
if(n==0){cout<<"sever"<<ends;}
}
if(c=='y'){
if(n==-1){cout<<"zapad"<<ends;}
if(n==1){cout<<"vostok"<<ends;}
if(n==0){cout<<"yug"<<ends;}
}
if(c=='z'){
